        },
        {
          "axisAlpha": 0,
          "position": "bottom",
          "reversed": false,
          "gridAlpha": 0,
          "labelsEnabled": false,
          "id": "raw_diff_press_axis",
          "autoGridCount": false,
        }
      ],
      "graphs": [
        {
          "lineColor": "#23c9ff",
          "lineThickness": 2,
          "type": "smoothedLine",
          "valueField": "pump_pressure",
          "valueAxis": "raw_pump_axis"
        },
        {
          "lineColor": "#9aea6a",
          "lineThickness": 2,
          "type": "smoothedLine",
          "valueField": "mud_volume",
          "valueAxis": "raw_mv_axis"
        },
        {
          "lineColor": "#9eddde",
          "lineThickness": 2,
          "type": "smoothedLine",
